{
  "name": "Voila",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Voila",
  "browser_action": {
    "default_icon": "images/icon.png",
	  "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["js/jquery.min.js", "js/jcanvas.min.js", "js/voila.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "tabs", "*://*.youtube.com/watch*"
  ]
}
